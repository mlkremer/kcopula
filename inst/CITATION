citHeader("To cite kcopula in publications use:")

citEntry(entry = "Manual",
  title        = "kcopula: The Bivariate K-Copula",
  author       = personList(as.person("Marcel Kremer")),
  year         = "2020",
  note         = "R package version 0.1.0",
  url          = "https://CRAN.R-project.org/package=kcopula",

  textVersion  =
  paste("Marcel Kremer (2020).",
        "kcopula: The Bivariate K-Copula.",
        "R package version 0.1.0.",
        "https://CRAN.R-project.org/package=kcopula")
)
